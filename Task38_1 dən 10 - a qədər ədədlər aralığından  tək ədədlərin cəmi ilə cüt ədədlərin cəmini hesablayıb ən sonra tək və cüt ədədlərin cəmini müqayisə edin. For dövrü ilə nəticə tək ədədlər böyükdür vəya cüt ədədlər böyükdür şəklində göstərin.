<?php
$tek = 0;
$cut = 0;

for ($i = 1; $i <= 10; $i++) {
    if ($i % 2 == 0) {
        $cut += $i;
    } else {
        $tek += $i; 
    }
}

echo "Tək ədədlərin cəmi: $tek<br>";
echo "Cüt ədədlərin cəmi: $cut<br>";

if ($tek > $cut) {
    echo "Tək ədədlərin cəmi cüt ədədlərin cəmindən böyüktür.";
} elseif ($cut > $tek) {
    echo "Cüt ədədlərin cəmi tək ədədlərin cəmindən böyüktür.";
} else {
    echo "Tək və cüt ədədlərin cəmi bərabərdir.";
}

?>
