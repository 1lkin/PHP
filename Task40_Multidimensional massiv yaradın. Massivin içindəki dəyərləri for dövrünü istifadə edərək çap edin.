<?php
$massiv = [
    ["alma", "armud", "banan"],
    ["pomidor", "badımcan", "biber"],
    ["toyuq eti", "mal eti", "balıq"]
];

for ($i=0; $i < count($massiv); $i++){
    echo "Alınacaqlar: ";
    for ($j=0; $j < count($massiv[$i]); $j++) {
        echo $massiv[$i][$j] . " ";
    }
    echo "<br>";
}

?>
